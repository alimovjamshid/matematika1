<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="com.google.android.gms.permission.AD_ID"/>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/screeen"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Matematika1"
        tools:targetApi="33">
        <activity
            android:name="com.math.matematika_1.activity.TestActivity"
            android:exported="false"
            android:parentActivityName="com.math.matematika_1.activity.PlayActivity" />
        <activity
            android:name="com.math.matematika_1.activity.PlayActivity"
            android:exported="false"
            android:parentActivityName="com.math.matematika_1.activity.MainActivity" />
        <activity
            android:name="com.math.matematika_1.activity.MainActivity"
            android:exported="true"
            android:theme="@style/Mytheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>